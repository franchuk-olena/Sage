    "G = DihedralGroup(4) \n",
    "\n",
    "print(\"Порядок групи G (D4):\", G.order())\n",
    "\n",
    "subgroups_order_3 = [H for H in G.subgroups() if H.order() == 3]\n",
    "\n",
    "if subgroups_order_3:\n",
    "    print(\"Підгрупи порядку 3 існують:\", subgroups_order_3)\n",
    "else:\n",
    "    print(\"Підгрупи порядку 3 не існує.\")\n"
